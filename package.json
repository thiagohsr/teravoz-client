{
  "name": "teravoz-callcenter",
  "version": "1.0.0",
  "description": "Client to integrate with Teravoz API",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --exec 'babel-node server.js' --ignore db.json",
    "jsonServer": "json-server --watch db.json --routes json-server-routes.js",
    "dev": "npm-run-all --parallel jsonServer start",
    "test:watch": "jest --watchAll --verbose",
    "test": "jest --verbose"
  },
  "author": "thiago ramos",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge",
    "babel-plugin-module-resolver": "^3.1.1",
    "express": "^4.16.4",
    "jest": "^23.6.0",
    "jest-mock-axios": "^2.1.11",
    "json-server": "^0.14.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
