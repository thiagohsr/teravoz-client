{
  "name": "teravoz-callcenter",
  "version": "1.0.0",
  "description": "Teravoz Challenge - Client to integrate with Teravoz API",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --exec 'babel-node server.js' --ignore db.json",
    "jsonServer": "json-server --watch db.json --routes json-server-routes.js",
    "dev": "npm-run-all --parallel jsonServer start",
    "test:watch": "jest --watchAll --verbose",
    "test": "jest --verbose",
    "lint": "node_modules/eslint/bin/eslint.js ."
  },
  "author": "thiago ramos",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "json-server": "^0.14.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jest-mock-axios": "^2.1.11"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
