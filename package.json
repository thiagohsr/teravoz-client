{
  "name": "teravoz-callcenter",
  "version": "1.0.0",
  "description": "Teravoz Challenge - Client to integrate with Teravoz API",
  "main": "server.js",
  "scripts": {
    "dashboard": "yarn dashboard:build && yarn dashboard:start",
    "dashboard:dev": "node_modules/next/dist/bin/next frontend/ -p 5000",
    "dashboard:build": "node_modules/next/dist/bin/next build frontend/",
    "dashboard:start": "node_modules/next/dist/bin/next start frontend/ -H 0.0.0.0 -p 5000",
    "api:teravoz": "nodemon --exec 'babel-node teravoz/server.js' --ignore jsonServer/db.json --ignore incoming_calls.db --ignore frontend/",
    "client": "nodemon --exec 'babel-node client/server.js' --ignore jsonServer/db.json --ignore incoming_calls.db --ignore frontend/",
    "jsonServer": "json-server --host 0.0.0.0 --watch jsonServer/db.json --routes jsonServer/json-server-routes.js",
    "dev": "npm-run-all --parallel jsonServer api:teravoz client dashboard:dev",
    "production": "yarn dashboard:build && npm-run-all --parallel jsonServer api:teravoz client dashboard:start",
    "test:watch": "jest --watchAll --verbose",
    "test": "NODE_ENV=test jest --verbose",
    "lint": "node_modules/eslint/bin/eslint.js ."
  },
  "author": "thiago ramos",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "babel-plugin-inline-dotenv": "^1.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "body-parser": "^1.18.3",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "express": "^4.16.4",
    "json-server": "^0.14.0",
    "lokijs": "^1.5.5",
    "next": "^7.0.2",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "jest-mock-axios": "^2.1.11"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
